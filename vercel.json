{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/approveBooking",
      "destination": "/api/approveBooking.js"
    },
    {
      "source": "/api/book",
      "destination": "/api/book.js"
    },
    {
      "source": "/api/deleteBooking",
      "destination": "/api/deleteBooking.js"
    },
    {
      "source": "/api/getBookings",
      "destination": "/api/getBookings.js"
    },
    {
      "source": "/api/login",
      "destination": "/api/login.js"
    },
       {
      "source": "/api/update-status",
      "destination": "/api/update-status.js"
    },
    {
      "source": "/api/sendMail",
      "destination": "/api/sendMail.js"
    }
  ]
}
